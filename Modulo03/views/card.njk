{% extends "layout.njk" %}

{% block head %}
    <title>{{item.titulo}}</title>

{% endblock %}

{% block content %}
    <div id="wrapper" class="post">
        <h1>{{item.titulo}}</h1>
    </div>

    <section class="post">

        <div class="card" id="{{item.id}}">
            <div class="card_post-container ">
                <iframe src="https://blog.rocketseat.com.br/{{item.id}}" frameboder="0"></iframe>

            </div>
            <div class="card_info">
                <p class="card_titulo">{{item.titulo}}</p>
                <p class="card_autor">{{item.autor}}
                </p>
                <p class="card_tempo">{{item.tempo}}
                </p>
            </div>
        </div>

    </section>

{% endblock %}